action general a_test named "test";

world
{
	onAction(a_test)
	{
		local i;
		local n = irandom(10);
		Textln("Loop will stop at " + n);
		for (i = 0; i < 10; i = i + 1)
		{
			if (i === n)
				break;
			Textln(i);
		}
		
		Textln("i is " + i);
	}

	start()
	{
		Textln("Type \"test\" to run the break example.");
	}
	
}