world
{
	start()
	{
		local a;
		a = 10;
		Textln("Generating " + a + " integers from 0 to 9:");
		while (a > 0)
		{
			Textln(irandom(10));
			a = a - 1;
		}
		Textln("");

		a = 10;
		Textln("Generating " + a + " floats from [0.0, 1.0):");
		while (a > 0)
		{
			Textln(frandom());
			a = a - 1;
		}
		Textln("");

		a = 10;
		Textln("Generating " + a + " Gaussian floats, mean 0, std. dev. 1:");
		while (a > 0)
		{
			Textln(grandom(0, 1));
			a = a - 1;
		}
		
		quit;
	}
}